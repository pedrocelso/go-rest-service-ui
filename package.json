    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-jsonschema-form": "^1.7.4",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.9.3",
    "@types/request-promise": "^4.1.46",
    "@types/sinon": "^9.0.8",
    "add": "^2.0.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
