{
  "name": "task-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bluebird": "^3.5.3",
    "bootstrap": "^4.3.1",
    "dotenv": "^7.0.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "fluture": "^11.0.1",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^4.11.0",
    "popper.js": "^1.15.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "request-promise": "^4.2.4",
    "sinon": "^7.1.0",
    "typescript": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.{js,jsx,ts,tsx}",
      "!src/setupTests.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/bluebird-global": "^3.5.11",
    "@types/bootstrap": "^4.3.0",
    "@types/dotenv": "^6.1.1",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "24.0.11",
    "@types/jquery": "^3.3.29",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/nock": "^9.3.1",
    "@types/node": "11.13.0",
    "@types/popper.js": "^1.11.0",
    "@types/ramda": "^0.26.6",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.8.1",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.0.11",
    "@types/typescript": "^2.0.0",
    "add": "^2.0.6",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "nock": "^10.0.6",
    "react-test-renderer": "^16.8.6",
    "ts-loader": "^5.3.3",
    "yarn": "^1.15.2"
  }
}
